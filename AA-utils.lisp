; util {{{
(defun fresh-princ(str)
  (princ str)
  (fresh-line))

(defmacro cadr-assoc (key node)
  `(cadr (assoc ,key ,node)))
; }}}
; logic {{{

(defun charlist->aa-print (charlist nodes)
  (fresh-line)
  (loop for i from 0 to 7
        collect (fresh-princ (aaline charlist i nodes))))
; (charlist->aa-print '(#\T #\e #\s #\t) *aa-nodes*)

(defun aaline (charlist line-num nodes)
  (let* ((aa-alist 
           (cadr-assoc (car charlist) nodes))
         (aa-string
           (cadr-assoc line-num  (symbol-value aa-alist))))
    (if charlist
      (concatenate 'string aa-string
                   (aaline (cdr charlist) line-num nodes))
      " ")))

(defun string->charlist (str)
  (if (equal "" str)
    (list #\Space)
    (concatenate 'list
                 (list (char str '0))
                 (string->charlist (substring str '1)))))
; (print (to-charlist "test"))
; "test" -> (#\t #\e #\s #\t #\Space) 
;}}}

(defun aa-print(str)
  (charlist->aa-print (string->charlist str) *aa-nodes*))

