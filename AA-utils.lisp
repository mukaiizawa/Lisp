                           (#\R  *aa-Lr*)
                           (#\S  *aa-Ls*)
                           (#\T  *aa-Lt*)
                           (#\U  *aa-Lu*)
                           (#\V  *aa-Lv*)
                           (#\W  *aa-Lw*)
                           (#\X  *aa-Lx*)
                           (#\Y  *aa-Ly*)
                           (#\Z  *aa-Lz*)
                           (#\. *aa-dot*)
                           (#\! *aa-exc*)
